 class Animal {
   
    void Walk(){
        System.out.println("Animal is walking");
   }
}
class dog extends Animal{
    void eat(){
        System.out.println("dog is eating");
    }
}
public  class inheritance{
    public static void main(String[] args) {
        dog d=new dog();
        d.eat();
        d.Walk();
        
    }
}

